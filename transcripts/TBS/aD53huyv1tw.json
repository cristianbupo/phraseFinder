  {
    "text": "YEAH?",
    "start": 169.36,
    "duration": 0.5
  },
  {
    "text": "IT'S NICE.",
    "start": 170.56,
    "duration": 0.56
  },
  {
    "text": "ARE YOU KIDDING ME?",
    "start": 173.68,
    "duration": 1.12
  },
  {
    "text": "IS THIS TURNING YOU ON?",
