[
  {
    "text": "You've been taking a cooking class.",
    "start": 0.29,
    "duration": 1.38
  },
  {
    "text": "Where do you think I go every Tuesday night?",
    "start": 1.7,
    "duration": 1.89
  },
  {
    "text": "You're not here on Tuesday nights?",
    "start": 3.62,
    "duration": 1.68
  },
  {
    "text": "No, I'm in cooking class.",
    "start": 7.01,
    "duration": 2.4
  },
  {
    "text": "And guess who's the teacher?",
    "start": 9.44,
    "duration": 1.56
  },
  {
    "text": "Who?",
    "start": 11.06,
    "duration": 0.75
  },
  {
    "text": "Sandy.",
    "start": 12.26,
    "duration": 1.23
  },
  {
    "text": "Oh.",
    "start": 13.58,
    "duration": 0.96
  },
  {
    "text": "Who's Sandy?",
    "start": 15.59,
    "duration": 1.17
  },
  {
    "text": "That's Sandy.",
