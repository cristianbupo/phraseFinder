[
  {
    "text": "Hello, Scratch.",
    "start": 0.433,
    "duration": 1.135
  },
  {
    "text": "I made too much salad for lunch.\nYou want some?",
    "start": 1.568,
    "duration": 2.135
  },
  {
    "text": "Nah, I'm good, [inaudible].",
    "start": 3.703,
    "duration": 1.201
